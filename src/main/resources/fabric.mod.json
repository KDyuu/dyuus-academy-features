	"description": "Features and Compatibilities for Dyuu's Cobblemon Academy",
	"authors": [
		"KDyuu"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
